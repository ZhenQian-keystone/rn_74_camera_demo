{
    "name": "rn_74_camera_demo",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "start": "react-native start",
        "test": "jest",
        "postinstall": "patch-package && ./node_modules/.bin/rn-nodeify --hack --install && npx jetify",
        "clean-start": "yarn start --reset-cache",
        "android-clean": "cd android && ./gradlew clean && cd ..",
        "ios-clean": "rm -rf ios/Pods && rm -rf ios/build",
        "pod-install": "cd ios && pod install && cd ..",
        "clean-install": "react-native-clean-project",
        "bump-patch": "npx react-native bump-version --skip-semver-for android --skip-code-for all --type patch",
        "bump-minor": "npx react-native bump-version --skip-semver-for android --skip-code-for all --type minor",
        "bump-major": "npx react-native bump-version --skip-semver-for android --skip-code-for all --type major",
        "prettier": "prettier --write *.ts*",
        "gen-types:all": "yarn gen-types",
        "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle"
    },
    "dependencies": {
        "@gorhom/bottom-sheet": "^4",
        "@react-native-async-storage/async-storage": "^1.24.0",
        "@react-native-community/blur": "^4.4.0",
        "@react-navigation/bottom-tabs": "^6.6.1",
        "@react-navigation/native": "^6.1.18",
        "@react-navigation/native-stack": "^6.11.0",
        "@react-navigation/stack": "^6.4.1",
        "@shopify/restyle": "^2.4.4",
        "expo": "^51.0.0",
        "expo-haptics": "~13.0.1",
        "expo-secure-store": "~13.0.2",
        "i18next": "^23.12.3",
        "react": "18.3.1",
        "react-async-hook": "^4.0.0",
        "react-i18next": "^15.0.1",
        "react-native": "0.75.3",
        "react-native-device-info": "^11.1.0",
        "react-native-gesture-handler": "^2.18.1",
        "react-native-reanimated": "^3.15.0",
        "react-native-safe-area-context": "^4.11.0",
        "react-native-screens": "^3.34.0",
        "react-native-svg": "15.2.0",
        "react-native-vision-camera": "^4.5.1",
        "tinycolor2": "^1.6.0",
        "use-debounce": "^10.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native/babel-preset": "0.75.3",
        "@react-native/eslint-config": "0.75.3",
        "@react-native/metro-config": "0.75.3",
        "@react-native/typescript-config": "0.75.3",
        "@types/react": "^18.2.6",
        "@types/react-test-renderer": "^18.0.0",
        "@types/tinycolor2": "^1.4.6",
        "babel-jest": "^29.6.3",
        "eslint": "^8.19.0",
        "jest": "^29.6.3",
        "prettier": "2.8.8",
        "react-native-svg-transformer": "^1.5.0",
        "react-test-renderer": "18.3.1",
        "typescript": "5.0.4"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "yarn@3.6.4"
}
